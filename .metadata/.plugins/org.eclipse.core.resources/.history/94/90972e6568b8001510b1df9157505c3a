
public class Person {
	String name;
	int age;
	
	public Person(String name, int age)
	{
		this.name = name;
		this.age = age;
	}
	public String toString()
	{
		return name + "("+age+"¼¼)";
	}
	public int hashCode()
	{
		return 1;
	}
	public boolean equals(Object obj)
	{
		Person cmp = (Person)obj;
	}
}
